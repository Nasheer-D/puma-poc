    "@types/jsonwebtoken": "7.2.3",
    "@types/moment-timezone": "0.5.1",
    "@types/morgan": "1.7.32",
    "@types/mysql": "2.15.0",
    "@types/node": "8.0.24",
    "@types/shortid": "0.0.29",
    "@types/winston": "2.3.5",
